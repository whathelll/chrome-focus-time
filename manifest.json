{
  "manifest_version": 2,
  "name": "Focus Time",
  "version": "1.0",
  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>", "storage"],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": { 
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
      "default_icon": "icons/icon128.png",
      "default_popup":"popup.html"
  }
}
